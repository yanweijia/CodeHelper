<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane layoutX="10.0" layoutY="10.0" prefHeight="387.0" prefWidth="639.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.codeconverter.HashConverterController">
    <center>
        <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="150" minWidth="100"
                                   prefWidth="133.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" onAction="#compute" text="calculate" GridPane.columnIndex="2"
                        GridPane.rowIndex="2"/>
                <Label text="hash:" GridPane.rowIndex="1"/>
                <TextArea fx:id="txtHash" editable="false" GridPane.columnIndex="1" GridPane.columnSpan="2"
                          GridPane.rowIndex="1"/>

                <ChoiceBox fx:id="cbSource" GridPane.columnIndex="1" GridPane.rowIndex="2"/>


                <Label text="origin text:"/>
                <TextArea fx:id="txtOrigin" GridPane.columnIndex="1" GridPane.columnSpan="2"/>
                <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="encode from:" GridPane.rowIndex="2"/>
            </children>

        </GridPane>
    </center>
    <bottom>

    </bottom>
</BorderPane>
