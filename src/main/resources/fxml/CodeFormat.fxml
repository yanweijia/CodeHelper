<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<SplitPane dividerPositions="0.15" prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 5.0"
           xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="controller.CodeFormatController">
    <items>
        <ScrollPane fx:id="scrollPane" maxWidth="150.0" minWidth="110.0" pannable="true" prefHeight="200.0"
                    prefWidth="200.0">
            <content>
                <VBox alignment="TOP_CENTER" prefHeight="240.0" prefWidth="66.0" spacing="10.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#HTMLFormatFunction" text="HTMLFormat"/>
                        <Button mnemonicParsing="false" onAction="#JSONFormatFunction" text="JSONFormat"/>
                        <Button mnemonicParsing="false" onAction="#XMLFormatFunction" text="XMLFormat"/>
                        <Button mnemonicParsing="false" onAction="#SQLFormatFunction" text="SQLFormat"/>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
        <StackPane fx:id="container" prefHeight="200.0" prefWidth="200.0"/>
    </items>
</SplitPane>
